<% if logged_in? %>
	<p>Welcome, <%= get_user_name %></p>
	<%= link_to "Logout", logout_path %>
<% else %>
	<%= form_tag('/sessions', method: :create) do %>
		<p>
			<%= label_tag :email %>
			<%= text_field_tag :email %>
		</p>
		<p>
			<%= label_tag :password %><br>
			<%= text_field_tag :password %>
		</p>
		<p>
	    <%= submit_tag "Login" %>
	  </p>
	<% end %>
<% end %>